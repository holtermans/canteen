/* pages/ucenter/dishes/dished.wxss */
page{
  background: #fff;
}
.container{
  background-color: #fff;
}
.custom-card{
  height: 100px;
}
.custom-thumb{
  height: 50px;
}
.van-card__header{
  height: 80%;
}
.footer {
  position: fixed;
  bottom: 45px;
  right: 20px;
}
.publish-button {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  right: 50rpx;
  bottom: 50rpx;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background-color: #fff;
  box-shadow: 0rpx 0rpx 101rpx 0rpx rgba(0, 0, 0, 0.08), 0rpx 12rpx 26rpx 0rpx rgba(0, 0, 0, 0.05);
  overflow: hidden;
  z-index: 999;
}
.publish-button-icon {
  width: 50%;
  height: 50%;
}